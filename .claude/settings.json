{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "permissions": {
    "allow": [
      "Bash(mkdir:*)",
      "Bash(python3:*)",
      "Bash(npm:*)",
      "Bash(task-master:*)",
      "Bash(git:*)",
      "Bash(supabase:*)",
      "Bash(find:*)",
      "Bash(ls:*)",
      "Bash(cp:*)",
      "Write",
      "Edit",
      "MultiEdit",
      "Read",
      "Grep",
      "Glob",
      "NotebookRead",
      "NotebookEdit",
      "WebFetch",
      "WebSearch",
      "TodoWrite",
      "Bash(chmod:*)",
      "Bash(touch:*)"
    ],
    "deny": []
  },
  "statusLine": {
    "type": "command",
    "command": ".claude/statusline.sh",
    "padding": 0
  }
}